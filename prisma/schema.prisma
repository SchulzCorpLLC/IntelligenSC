generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Extension {
  id            String   @id @default(cuid())
  name          String
  description   String
  installs      Int
  rating        Float
  category      String
  keywords      String[]
  enrichedData  Json?
  updatedAt     DateTime @updatedAt
}
